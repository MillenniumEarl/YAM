{
    "main": "./app/app.ts",
    "name": "yam",
    "version": "3.0.0",
    "author": {
        "name": "Millennium Earl"
    },
    "description": "Unoffical Game Updater for the F95Zone platform",
    "repository": {
        "type": "git",
        "url": "https://github.com/MillenniumEarl/YAM.git"
    },
    "license": "MIT",
    "keywords": [
        "game",
        "games",
        "updater",
        "f95zone",
        "f95"
    ],
    "scripts": {
        "prettify": "prettier --config .prettierrc {app,test}/**/*.ts --write",
        "build": "tsc",
        "example": "npm run build && electron dist/app.js",
        "dist:win": "npm run build && electron-builder --win --config",
        "dist:mac": "npm run build && electron-builder --mac --config",
        "dist:linux": "npm run build && electron-builder --linux --config"
    },
    "engines": {
        "node": ">=16.0"
    },
    "dependencies": {
        "@millenniumearl/f95api": "^2.0.0-beta.10.1",
        "@millenniumearl/recaptcha-harvester": "^1.2.1",
        "catch-decorator-ts": "^0.0.2",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.1",
        "electron-store": "^8.0.1",
        "electron-updater": "^4.6.0",
        "error-stack-parser": "^2.0.6",
        "glob": "^7.2.0",
        "i18next": "^21.3.2",
        "i18next-electron-language-detector": "0.0.10",
        "image-downloader": "^4.0.3",
        "log4js": "^6.3.0",
        "lowdb": "^3.0.0",
        "new-github-issue-url": "^1.0.0",
        "source-map-support": "^0.5.20",
        "string-similarity": "^4.0.4"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.15.8",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "electron": "^15.2.0",
        "electron-builder": "^22.14.4",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.1"
    }
